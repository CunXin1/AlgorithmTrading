datasource db {
  provider = "postgresql"
}

generator client {
  // Prisma 7 新默认：建议用 prisma-client（而不是 prisma-client-js）
  provider = "prisma-client"
  // 相对 schema.prisma 的路径 => src/db/generated/prisma
  output   = "../generated/prisma"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
